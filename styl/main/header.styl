header
	padding 15px 0
	background-color: #4a175e
	overflow hidden
#logo
	float left
.contacts
	float: right
	line-height: 36px
	text-align: right
.contact
	color #fff
	font-family: "Arial", sans-serif
	margin-left: 15px
	span
		padding-left: 10px
	&:first-of-type
		margin-left: 0
	&:focus
		color #fff
		text-decoration: none
	&:hover
		color #fff
		text-decoration: underline