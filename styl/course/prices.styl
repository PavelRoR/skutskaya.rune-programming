#prices
	padding 50px 0
	background-color: #c5b4c2;
	background-image: url("../img/last_bg.jpg");
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	text-align: center;
.packet
	border-radius: 10px
	width 100%
	max-width 320px
	margin 0 auto 25px
	background-color: #4a175e
	text-align: center
	padding 20px
	box-shadow: 5px 5px 0px 0px rgba(#4a175e,0.45)
.packet_head
	padding 0 0 20px
.packet_head_title
	font-size: 22px
	font-family: 'OpenSansExtraBold', sans-serif
	text-transform: uppercase
	color #fff
	small
		color #d6c9e0
		display block
		font-size: 60%
		text-transform: none
		font-family: 'OpenSans', sans-serif
.packet_body
	padding 20px 0
	border-top: 1px solid #d6c9e0
	border-bottom: 1px solid #d6c9e0
	min-height 144px
.packet_body_title
	font-size: 16px
	font-family: 'OpenSans', sans-serif
	margin-bottom: 10px
	color #eee
.more_button
	color #d6c9e0
	font-size: 14px
	border-bottom: 1px dotted
	display: inline-block
	cursor pointer
.packet_footer
	padding 20px 0 0
.cost
	font-size: 22px
	font-family: 'OpenSansBold', sans-serif
	color #fff
	margin-bottom: 20px
	span
		display block
		font-family: 'OpenSansExtraBold', sans-serif
		font-size: 26px
.prepayment
	width 100%
	max-width 650px
	margin 0 auto
	text-align: center
.prepaymant_title
	font-size: 32px
	font-family: "OpenSansExtraBold", sans-serif
	color red
.prepayment_text
	font-size:   20px
	color #000
	margin 15px 0
.timer_title
	font-size: 32px
	text-align: center
	color #f00
	margin: 30px 0 0
	font-family: "OpenSansExtraBold",sans-serif
.alert-danger
	color: #a94442;
	background-color: #f2dede;
	padding: 10px;
	width 100%
	max-width 360px
	margin 20px auto 0
	border: 1px solid #ebccd1;
	border-radius: 4px;
	font-size: 22px;
	text-align: center
.flip-clock-wrapper
	width: 100%
	max-width 460px
.flip-clock-divider .flip-clock-label
	color #000
.flip-clock-dot
	background-color: #0056c2
.flip-clock-wrapper ul li a div div.inn
	color: #fff
	background-color: #0056c2
	text-shadow: 2px 2px 2px rgba(#022a37,.6);
.garants
	width 100%
	max-width 650px
	margin 40px auto 0
	background-color: rgba(255,255,255,.7)
	border-radius: 5px
	border: 1px dashed #000
	box-shadow 0 3px 15px 0 rgba(0,0,0,0.4)
	text-align: center
	padding 20px
	color #000
.garants_title
	font-size: 18px
	margin-bottom: 5px
	font-family: "OpenSansBold", sans-serif
.garants_text
	font-size: 16px
.general_title_recs
	margin 50px 0 15px
	line-height: 1.3
	span
		display inline-block
.buy_recs_text
	color #000
	text-align: center
	font-size: 22px
.buy_recs_button
	margin 20px auto 0