@font-face {
	font-family: 'OpenSans';
	src: url('../fonts/OpenSans.eot');
	src: url('../fonts/OpenSans.eot') format('embedded-opentype'),
		 url('../fonts/OpenSans.woff2') format('woff2'),
		 url('../fonts/OpenSans.woff') format('woff'),
		 url('../fonts/OpenSans.ttf') format('truetype'),
		 url('../fonts/OpenSans.svg#OpenSans') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'OpenSansBold';
	src: url('../fonts/OpenSansBold.eot');
	src: url('../fonts/OpenSansBold.eot') format('embedded-opentype'),
		 url('../fonts/OpenSansBold.woff2') format('woff2'),
		 url('../fonts/OpenSansBold.woff') format('woff'),
		 url('../fonts/OpenSansBold.ttf') format('truetype'),
		 url('../fonts/OpenSansBold.svg#OpenSansBold') format('svg');
	font-display: fallback;
	font-weight: bold;
	font-style: normal;
}
@font-face {
	font-family: 'OpenSansExtraBold';
	src: url('../fonts/OpenSansExtraBold.eot');
	src: url('../fonts/OpenSansExtraBold.eot') format('embedded-opentype'),
		 url('../fonts/OpenSansExtraBold.woff2') format('woff2'),
		 url('../fonts/OpenSansExtraBold.woff') format('woff'),
		 url('../fonts/OpenSansExtraBold.ttf') format('truetype'),
		 url('../fonts/OpenSansExtraBold.svg#OpenSansExtraBold') format('svg');
	font-display: fallback;
	font-weight: bold;
	font-style: normal;
}

@import '../css/_css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-family: 'OpenSans', sans-serif
	font-size: 16px
	background-color: rgb(220,202,226)
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video_wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	cursor pointer
	border-radius: 20px;
	background-color: rgb(255, 0, 0);
	width 100%
	max-width: 240px;
	height: 54px;
	line-height: 54px;
	font-family: "OpenSansBold", sans-serif;
	color: rgb(255, 255, 255);
	text-align: center;
	transition: all .3s ease
	text-decoration: none
	&:focus
		text-decoration: none
		color rgb(255,255,255)
	&:hover
		text-decoration: none
		color rgb(255,255,255)
		background-color: rgb(237,77,77)
.general_title
	text-align: center
	font-size: 24px;
	font-family: "OpenSansExtraBold", sans-serif
	color: rgb(74, 23, 94);
	text-transform: uppercase;
	line-height: 1.083;
	margin-bottom: 45px
.general_title_white
	color rgb(214,201,224)

